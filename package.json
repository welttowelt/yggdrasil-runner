{
  "name": "ls2-runner",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "start": "tsx src/index.ts",
    "start:headless": "RUNNER_HEADLESS=1 tsx src/index.ts",
    "flow:mainnet": "tsx src/tools/mainnetPlaywrightFlow.ts",
    "analyze:starknet": "python3 ${CODEX_HOME:-$HOME/.codex}/skills/starknet-debug/scripts/parse_starknet_error.py data/events.jsonl --tail 300",
    "analyze:clickflow": "python3 ${CODEX_HOME:-$HOME/.codex}/skills/cartridge-controller/scripts/check_click_order.py data/events.jsonl --tail 400",
    "analyze:health": "python3 ${CODEX_HOME:-$HOME/.codex}/skills/runner-observability/scripts/runner_health_report.py --events data/events.jsonl --milestones data/milestones.jsonl --tail 500",
    "analyze:recovery": "python3 ${CODEX_HOME:-$HOME/.codex}/skills/chaos-recovery/scripts/recovery_audit.py data/events.jsonl --tail 800",
    "probe": "tsx src/probe.ts",
    "lint": "node -e \"console.log('no lint configured')\""
  },
  "dependencies": {
    "playwright": "^1.42.0",
    "starknet": "^9.2.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^20.11.30",
    "tsx": "^4.7.1",
    "typescript": "^5.4.2"
  }
}
